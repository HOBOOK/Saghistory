			nounscatter();
		};
		
		$(document).ready(function(){
		    $(".menu>a").mouseover(function(){
		        var submenu = $(this).next("ul");
		        if( submenu.is(":visible") ){
		            submenu.slideUp();
		        }else{
		            submenu.slideDown();
		        }
		    });
		   
		});
	
		// addEventListener 이벤트
		window.addEventListener('load', function(){
		    setTimeout(scrollTo, 1);
		}, false);
		
		function nounscatter()
		{
			var buc = document.getElementsByClassName('bucketcov');
			var bucp = document.getElementsByClassName('bucketp');
			for(var i = 0; i < buc.length; i++){
				var r = Math.floor(Math.random()*255)+1;
				var g = Math.floor(Math.random()*255)+1;
				var b = Math.floor(Math.random()*255)+1;
				var ran2 = Math.floor(Math.random()*20)+10;
				//buc[i].style.transform="rotate("+ran+"deg)";
				bucp[i].style.color='rgba('+r+','+g+','+b+',1)';
				bucp[i].style.fontSize=ran2+'px';
				
			}
		}
		function checklength()
		{
			var str = document.getElementById("wr_reply").value;
			
			if(str.length<5)
			{
				alert("5자 이상 입력");
				document.getElementById("wr_reply").value = "";
				document.getElementById("wr_reply").focus();
				return false;
			}
			return true;
		}
		function arrange()
		{
			var buc = document.getElementsByClassName('bucketcov');
			var bucp = document.getElementsByClassName('bucketp');
			
			if(buc[0].style.display=="block"){
				nounscatter();
				for(var i = 0; i < buc.length; i++){
					buc[i].style.display="inline-block";
				}
			}
			else{
				for(var i = 0; i < buc.length; i++){
					buc[i].style.display="block";
					buc[i].style.transform="rotate(0deg)";
					bucp[i].style.fontSize="1rem";
					
				}
			}
			
		}
		function showTabMenu(n){
			var conid;
			var tabid;
			for(var i = 1; i < 4; i++){
				conid = document.getElementById("con"+i);
				tabid = document.getElementById("tab"+i);
				if(i==n){
					conid.style.display="block";
					tabid.style.borderBottom="2px solid rgba(0,80,255,1)";
					tabid.style.color="rgba(0,80,255,1)";
				}
				else{
					conid.style.display="none";
					tabid.style.border="none";
					tabid.style.color="rgba(0,0,0,0.7)";
				}
			}
			
		}
		
		$( ".star_rating a" ).click(function() {

		     $(this).parent().children("a").removeClass("on");

		     $(this).addClass("on").prevAll("a").addClass("on");

		     return false;

		});

	</script>
    
    
