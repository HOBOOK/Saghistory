	<script>
		window.onload = function() {
		    setTimeout(function() {
		        window.scrollTo(0, 1);}, 100);
		    
		    var tabid = document.getElementById("tab1");
		    tabid.style.borderBottom="2px solid rgba(0,80,255,1)";
